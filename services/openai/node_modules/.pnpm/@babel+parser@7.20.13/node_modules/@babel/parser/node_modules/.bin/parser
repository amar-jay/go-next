#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/manan/code/go/src/github.com/amar-jay/go_auth/services/openai/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/manan/code/go/src/github.com/amar-jay/go_auth/services/openai/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/babel-parser.js" "$@"
else
  exec node  "$basedir/../../bin/babel-parser.js" "$@"
fi
